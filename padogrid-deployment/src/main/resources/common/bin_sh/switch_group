SCRIPT_DIR="$(cd -P -- "$(dirname -- "$0")" && pwd -P)"
. $PADOGRID_HOME/$PRODUCT/bin_sh/.addonenv.sh

# Inexecutable - only for generting man page.
if [ "true" == "false" ]; then
cat <<EOF
NAME
   $EXECUTABLE - Switch to the specified group in the current workspace
EOF
fi

EXECUTABLE="`basename $0`"

# Delegate to function
"$EXECUTABLE" "$@"
