        "description": "$Cluster: PN Counter creation time and operation counts",
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "custom": {
              "align": "auto",
              "cellOptions": {
                "type": "auto"
              },
              "inspect": false
            },
            "mappings": [
              {
                "options": {
                  "pattern": "^.*(=)(\\w.*).*\"$",
                  "result": {
                    "index": 0,
                    "text": "$2"
                  }
                },
                "type": "regex"
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
